@{
    ViewBag.Title = "Home Page";
}
<style>
    a span {
        font-weight: bold;
        font-size: 25px;
    }

    uic
    /*.icono{
            color:green;
        }*/
    a i {
        font-size: 40px;
    }

    ﻿.fieldset-maquinas {
        display: block;
        margin-left: 5px;
        margin-right: 5px;
        padding-top: 10px;
        padding-bottom: 0.625em;
        padding-left: 0.999em;
        padding-right: 0.625em;
        border: 1px solid;
    }

    fieldset {
        border: 1px solid;
    }

    legend {
        margin-left: 5px;
    }

    .btn-purple {
        background-color: rebeccapurple;
        color: white;
    }

        .btn-purple:hover {
            background-color: purple;
            color: white;
        }

    .btn-dark i {
        color: red;
    }

    span {
        text-decoration: none;
        color: white;
    }
</style>

<div class="col-lg-12">

    <div id="maquinas"></div>

</div>

<!-- Modal -->
<div class="modal fade" id="seleccionModal" tabindex="-1" role="dialog" aria-labelledby="asignarModalLabel" aria-hidden="true" data-controls-modal="seleccionModal" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog" role="document">
        <div class="modal-content" style="background-color: #CECECE;color:black;">
            <div class="modal-header">
                <h5 class="modal-title" id="asignarModalLabel">TM System / Selección de maquina</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" name="inlineRadioOptions" runat="server" id="radioPM" value="PM" checked>
                        <label class="form-check-label" for="radioPM">Paro de maquina</label>
                    </div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" name="inlineRadioOptions" runat="server" id="radioAP" value="AP">
                        <label class="form-check-label" for="radioAP">Aviso preventivo</label>
                    </div>
                </div>

                <fieldset style="padding:15px;" runat="server" id="formLogin">
                    <div>
                        <div class="form-group">
                            <label for="txtNumeroEmpleado" style="font-size:16px;"><b>Numero de empleado:</b></label>
                            <input type="text" class="form-control" id="txtNumeroEmpleado" runat="server" placeholder="#######" />
                        </div>
                        <div class="form-group">
                            <label for="txtPassword" style="font-size:16px;"><b>Contraseña:</b></label>
                            <input type="password" class="form-control" id="txtPassword" runat="server" placeholder="**********" />
                        </div>
                    </div>
                </fieldset>
                <div class="modal-footer">
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Cancelar</button>
                    <button type="button" class="btn btn-warning" runat="server" id="btnAceptar" onserverclick="btnAceptar_ServerClick">Aceptar</button>
                </div>
            </div>
        </div>
    </div>
</div>


<!-- Modal -->
<div class="modal fade" id="atenderModal" tabindex="-1" role="dialog" aria-labelledby="atenderModal" aria-hidden="true" data-controls-modal="atenderModal" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog" role="document">
        <div class="modal-content" style="background-color: #CECECE;color:black;">
            <div class="modal-header">
                <h5 class="modal-title" id="atenderModalLabel">TM BRAZING / Atender</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <input type="text" class="form-control" id="txtReporte" runat="server" hidden="hidden" readonly="readonly" />

                <fieldset style="padding:15px;" runat="server" id="Fieldset1">
                    <div>
                        <div class="form-group">
                            <label for="txtNumeroEmpleado" style="font-size:16px;"><b>Numero de empleado:</b></label>
                            <input type="text" class="form-control" id="txtNsumeroEmpleadoAtender" runat="server" placeholder="#######" />
                        </div>
                        <div class="form-group">
                            <label for="txtPassword" style="font-size:16px;"><b>Contraseña:</b></label>
                            <input type="password" class="form-control" id="txtPasswordAtender" onkeypress="if (event.keyCode == 13) ejecutar();" runat="server" placeholder="**********" />
                        </div>


                    </div>

                </fieldset>
                <div class="modal-footer">
                    <button type="button" class="btn btn-warning" runat="server" id="btnAceptarAtender" onserverclick="btnAceptarAtender_ServerClick">Aceptar</button>
                </div>
            </div>
        </div>
    </div>
</div>


<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
